<!DOCTYPE project>
<project name="init" basedir=".">

    <target name="init">
        <mkdir dir="${libs.dir}" />
        <mkdir dir="${build.tmp.dir}" />
        <mkdir dir="${dist.tmp.dir}" />
        <mkdir dir="${reports.tmp.dir}" />
        <mkdir dir="${javadoc.tmp.dir}" />
        <tstamp>
            <format property="build.date" pattern="yyMMddHHmmss" />
        </tstamp>
    </target>

</project>
